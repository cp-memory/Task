class MinStack:

    def __init__(self):
        self.obj = []
        """
        initialize your data structure here.
        """
        

    def push(self, x: int) -> None:
        self.obj.append(x)
        

    def pop(self) -> None:
        return self.obj.pop()
        

    def top(self) -> int:
        return self.obj[-1]
        

    def getMin(self) -> int:
        return min(self.obj)
        


# Your MinStack object will be instantiated and called as such:
# obj = MinStack()
# obj.push(x)
# obj.pop()
# param_3 = obj.top()
# param_4 = obj.getMin()
